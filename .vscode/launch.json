    {
      "type": "node",
      "request": "launch",
      "name": "fm-gui main process",
      "env": {
        "NODE_ENV": "development",
        "ELECTRON_DISABLE_SECURITY_WARNINGS": "true"
      },
      "preLaunchTask": "task:renderer",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "runtimeArgs": [
        "--remote-debugging-port=9223",
        "-r",
        "ts-node/register",
        "${workspaceFolder}/src/main/main.ts"
      ]
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "fm-gui renderer process",
      "port": 9223,
      "webRoot": "${workspaceFolder}"
    },
  ],
  "compounds": [
    {
      "name": "fm-gui renderer/main process",
      "configurations": ["fm-gui renderer process", "fm-gui main process"]
    }
  ]
}